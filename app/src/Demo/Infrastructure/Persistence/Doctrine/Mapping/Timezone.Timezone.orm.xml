<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="Demo\Infrastructure\Persistence\Doctrine\TimezoneDoctrineRepository" name="Demo\Domain\Model\Timezone\Timezone" table="Timezones" change-tracking-policy="DEFERRED_EXPLICIT">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
      <options>
        <option name="unsigned">1</option>
      </options>
    </id>
    <one-to-many field="administrators" target-entity="Demo\Domain\Model\Administrator\AdministratorInterface" mapped-by="timezoneId" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
