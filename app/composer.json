{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "url": "https://github.com/irontec/TestDoubleBundle.git",
            "type": "git"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "chillerlan/php-qrcode": "^4.3",
        "doctrine/dbal": "^3.2",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.11",
        "doctrine/persistence": "^2.1",
        "gesdinet/jwt-refresh-token-bundle": "^0.12",
        "irontec/ivoz-api-bundle": "^4.7",
        "irontec/ivoz-core": "dev-CDD-17-mark-entitytools-methods-as-internal as 4.18",
        "irontec/ivoz-core-bundle": "^4.7.1",
        "irontec/ivoz-dev-tools": "^5.12.1",
        "symfony/asset": "^5.4",
        "symfony/cache": "^5.4",
        "symfony/console": "^5.4",
        "symfony/debug-bundle": "^5.4",
        "symfony/doctrine-bridge": "^5.4",
        "symfony/dom-crawler": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/error-handler": "^5.4",
        "symfony/event-dispatcher": "^5.4",
        "symfony/expression-language": "^5.4",
        "symfony/filesystem": "^5.4",
        "symfony/finder": "^5.4",
        "symfony/flex": "^1.3.1",
        "symfony/form": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/http-client": "6.0.*",
        "symfony/http-foundation": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/mailer": "^5.4",
        "symfony/mime": "^5.4",
        "symfony/monolog-bridge": "^5.4",
        "symfony/options-resolver": "^5.4",
        "symfony/password-hasher": "^5.4",
        "symfony/property-access": "^5.4",
        "symfony/property-info": "^5.4",
        "symfony/proxy-manager-bridge": "^5.1",
        "symfony/routing": "^5.4",
        "symfony/runtime": "^5.4",
        "symfony/security-bundle": "^5.4",
        "symfony/security-core": "^5.4",
        "symfony/security-csrf": "^5.4",
        "symfony/security-http": "^5.4",
        "symfony/serializer": "^5.4",
        "symfony/stopwatch": "^5.4",
        "symfony/string": "^5.4",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^5.4",
        "symfony/twig-bridge": "^5.4",
        "symfony/twig-bundle": "^5.4.3",
        "symfony/validator": "^5.4",
        "symfony/var-dumper": "^5.4",
        "symfony/var-exporter": "^5.4",
        "symfony/web-link": "^5.4",
        "symfony/yaml": "^5.4",
        "twig/twig": "^3.3",
        "zordius/lightncandy": "^1.2"
    },
    "require-dev": {
        "behat/mink": "^1.10",
        "behatch/contexts": "^3.3",
        "dms/phpunit-arraysubset-asserts": "^0.4.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "friends-of-behat/mink-browserkit-driver": "^1.6",
        "friends-of-behat/mink-extension": "^2.6",
        "friends-of-behat/symfony-extension": "^2.3",
        "phpstan/phpstan": "^0.12.81",
        "phpstan/phpstan-beberlei-assert": "^0.12.6",
        "phpstan/phpstan-doctrine": "^0.12.33",
        "phpstan/phpstan-symfony": "^0.12.21",
        "phpunit/phpunit": "^9.5.3",
        "squizlabs/php_codesniffer": "^3.6.2",
        "symfony/phpunit-bridge": "^5.1"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "./tests"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan -c phpstan.neon",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0.*"
        }
    }
}
